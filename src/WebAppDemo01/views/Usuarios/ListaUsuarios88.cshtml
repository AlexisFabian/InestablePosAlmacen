@model ListaUsuariosViewModel

<div class="container">
    <h2>Perfil de Usuarios del Sistema</h2>
    <p>Lista Usuarios</p>
    <table class="table table-striped">
        <thead>
            <tr>
            <tr>
                <th>ID</th>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Cargo</th>
                <th>Clave</th>
                <th>DescripTrabajo</th>
                <th>Activo</th>
                <th>Inactivo</th>
                <th>CodAreaTrabajo</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var usuarios in Model.Usuarios)
            {
                @Html.Partial("DesplegarUsuarios", usuarios)

            }

        </tbody>
    </table>
</div>



